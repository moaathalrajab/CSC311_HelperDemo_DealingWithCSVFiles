<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="600.0" prefWidth="500.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/17.0.2-ea"  fx:controller="com.example.csc311_helperdemo_dealingwithcsvfiles.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <TextArea fx:id="areaText" prefHeight="200.0" prefWidth="200.0" />

    <Label fx:id="welcomeText" />
    <Button onAction="#onButtonClick" text="Hello!" />
    <Button onAction="#onOpenButtonClick" text="Open!" />

    <Button onAction="#onSaveButtonClick" text="Save!" />

    <Button onAction="#onSavePDFButtonClick" text="Save PDF!" />
</VBox>
